          <BodyContent xsi:type="ST">
            <ST>// Dear Phoenix Conact... I love you but please make an actual rand block &lt;3

Seed := 12345;  // Seed value set to runtime
A := 1664525;   // Multiplier (adjust as needed)
C := 1013904223;  // Increment (adjust as needed)
M := 4294967296;  // Modulus (2^32)

IF Enable THEN
// Calculate next state using LCG formula
    X := (A * X + C) MOD M;

    // Scale the result to the desired range
    RandomValue := TO_INT(TO_ULINT(MinLimit) + X MOD (TO_ULINT(MaxLimit) - TO_ULINT(MinLimit) + 1));
END_IF;
</ST>
          </BodyContent>
